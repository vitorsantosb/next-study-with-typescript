{
  "extends": "next/core-web-vitals",
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-case-declarations": "off"
  }
}
